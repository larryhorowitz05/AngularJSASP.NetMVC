                                   new { @class = "closingCostTextBoxAmountLeft rightClick Amount Calc1101TotalAmount CalculateFromBorrowerFunds" + ( costGroup.Costs.First( c => c.HUDLineNumber == 1107 ).IsAprCost ? " blue" : "" ) + ( costGroup.Costs.First( c => c.HUDLineNumber == 1107 ).IsLocked ? " locked" : String.Empty ), @id = "txtAmount1107" } )
                </div>
                <div class="closingCostGfeRight" style="margin-right: -18px;">
                    <span class="closingCostText">(GFE #4)</span><span class="lockImageSection"></span>
                </div>
            </div>
            <div class="closingCostSection1100Right" style="height: 35px;">
                <div class="closingCostSectionRight1100WithoutAmount">
                    @Html.Partial( closingCostSectionRightWithoutAmountLocation, Model, new ViewDataDictionary { { "GroupNumber", 11 }, { "HUDLineNumber", 1107 } } )
                    @Html.Partial( closingcostsPOCDropdownsLocation, Model, new ViewDataDictionary { { "GroupNumber", 11 }, { "HUDLineNumber", 1107 } } )
                </div>
