        costGroup = Model.TitleAndEscrow.CostGroups.First( c => c.CostGroupNumber == 12 );
        costPrefix = "TitleAndEscrow.CostGroups[" + Model.TitleAndEscrow.CostGroups.IndexOf( costGroup ) + "].Costs";

        var htmlAttrForRecordingFeesAndTaxesClass = "closingCostTextBoxAmountLeft Amount CalculateFromBorrowerFunds rightClick";
