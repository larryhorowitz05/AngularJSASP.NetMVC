                    @Html.TextBox( String.Format( "{0}[{1}].Provider", costPrefix, costGroup.Costs.FindIndex( c => c.HUDLineNumber == 1108 ) ),
                                   costGroup.Costs.First( c => c.HUDLineNumber == 1108 ).Provider, new
