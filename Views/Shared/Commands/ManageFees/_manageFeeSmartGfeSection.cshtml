                    @Html.TextBox( String.Format( "{0}[{1}].Provider", costPrefix, costGroup.Costs.FindIndex( c => c.HUDLineNumber == 1102 ) ),
                                   costGroup.Costs.First( c => c.HUDLineNumber == 1102 ).Provider, new { @class = "closingCostTextBoxLarge281" } )
                </div>
                <div class="closingCostTextBoxDiv">
                    @Html.TextBox( String.Format( "{0}[{1}].Amount", costPrefix, costGroup.Costs.FindIndex( c => c.HUDLineNumber == 1102 ) ),
                                   DecimalExtensionMethods.ToCurrencyFormat( costGroup.Costs.First( c => c.HUDLineNumber == 1102 ).Amount ),
                                   new { @class = "closingCostTextBoxAmountLeft Amount Calc1101TotalAmount rightClick CalculateFromBorrowerFunds" + ( costGroup.Costs.First( c => c.HUDLineNumber == 1102 ).IsAprCost ? " blue" : "" ) + ( costGroup.Costs.First( c => c.HUDLineNumber == 1102 ).IsLocked ? " locked" : String.Empty ), @id = "txtAmount1102" } )
