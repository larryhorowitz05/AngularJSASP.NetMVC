                    @Html.Partial( closingcostsPOCDropdownsLocation, Model, new ViewDataDictionary { { "GroupNumber", 11 }, { "HUDLineNumber", 1108 } } )
                </div>
            </div>
            @for ( int hudLineNumber = 1109; hudLineNumber <= 1116; hudLineNumber++ )
            {
                var cost = costGroup.Costs.FirstOrDefault( c => c.HUDLineNumber == hudLineNumber );
                if ( cost == null )
                {
                    continue;
