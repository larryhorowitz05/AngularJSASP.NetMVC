@model MML.Web.LoanCenter.ViewModels.OrderDeliveredForReviewViewModel
@using MML.Contracts
@using Telerik.Web.Mvc.UI
@using MML.Common
@using MML.Common.Helpers
@using MML.Web.LoanCenter.Helpers.Utilities
<div class="todolist">
    <div id="todolisthead" class="todolisthead">
        <div class="left0" style="width: 210px">
            <div class="fleft">
                <span id="currentWorkQueueType" class="namelink">Order Delivered for Review</span></div>
            <div class="smallarrow">
            </div>
        </div>
        <div class="nooftasks">
            <span id="nooftasks2">@Model.TotalItems.ToString()</span> <span class="font15">Order
                Delivered for Review</span>
        </div>
        <div class="right1">
            <div class="left1">
                <span class="normalfont">Show: </span><span id="officerTaskHeadderSpan" class="g12">@MML.Common.Helpers.EnumHelper.GetStringValue( ( ( MML.Web.LoanCenter.Helpers.Utilities.OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).BoundDate )</span>
            </div>
            <div class="right0">
                <div class="filter">
                    <ul id="nav">
                        <li><a href="#">&nbsp;</a>
                            <ul>
                                <li></li>
                                <li id="ftoday" class="lower">@Ajax.ActionLink( "Today", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridFilter,DateFilter=1" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Today')" }, new { id = "aftoday" } )</li>
                                <li id="ftomorrow" class="lower">@Ajax.ActionLink( "Yesterday", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridFilter,DateFilter=2" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Tomorrow')" }, new { id = "aftomorrow" } )</li>
                                <li id="fthisweek" class="lower">@Ajax.ActionLink( "Last Week", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridFilter,DateFilter=3" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('This Week')" }, new { id = "afthisweek" } )</li>
                                <li id="fnextweek" class="lower">@Ajax.ActionLink( "Last Month", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridFilter,DateFilter=4" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Next Week')" }, new { id = "afnextweek" } )</li>
                                <li id="fthismonth" class="lower">@Ajax.ActionLink( "All Open", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridFilter,DateFilter=5" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('This Month')" }, new { id = "afthismonth" } )</li>
                                <li></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <a id="minimizetoday">
                    <div id="minmax" class="min">
                    </div>
                </a>
            </div>
        </div>
    </div>
    <div id="listandpage">
        <div class="queuecontent">
            <table class="queuetable">
                <tr class="queuesubjects" id="trGridHeader">
                    <td>
                    </td>
                    <td class="t0">
                    </td>
                    <td style="display: none">
                    </td>
                    <td class="t">@Ajax.ActionLink( "Loan #", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=1" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('LoanNumber')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('LoanNumber')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('LoanNumber')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('LoanNumber')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Borrower", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=2" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Borrower')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Borrower')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Borrower')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Borrower')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Purpose", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=3" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Purpose')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Purpose')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Purpose')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Purpose')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Loan Amt", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=4" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('LoanAmount')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('LoanAmount')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('LoanAmount')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('LoanAmount')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Purch Amt", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=5" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('PurchaseAmount')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('PurchaseAmount')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('PurchaseAmount')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('PurchaseAmount')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Est Value", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=6" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('EstimatedValue')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('EstimatedValue')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('EstimatedValue')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('EstimatedValue')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "OnLine", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=7" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('OnLine')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('OnLine')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('OnLine')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('OnLine')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Value Supported", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=8" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('ValueSupported')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('ValueSupported')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('ValueSupported')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('ValueSupported')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "eConsented", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=9" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('eConsented')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('eConsented')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('eConsented')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('eConsented')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "eDelivered", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=10" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('eDelivered')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('eDelivered')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('eDelivered')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('eDelivered')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Uploaded Date", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=11" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('UploadedDate')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('UploadedDate')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('UploadedDate')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('UploadedDate')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Delivered Date", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=12" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('DeliveredDate')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('DeliveredDate')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('DeliveredDate')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('DeliveredDate')" } )
                    </td>
                    <td class="t">@Ajax.ActionLink( "Age", "Execute", "Command", new { Command = "OrderDeliveredForReviewGridSorting,Column=13" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Age')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Age')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Age')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Age')" } )
                    </td>
                </tr>
                @if ( Model.DeliveredForReviewOrders != null )
                {
                    for ( int i = 0; i < Model.DeliveredForReviewOrders.Count; i++ )
                    {
                    <tr class="@Model.DeliveredForReviewOrders[ i ].ClassCollection first last"  id="task_@{@String.Format( "{0}P", i )}" onclick="SelectedTask('@String.Format( "{0}P", i )','@Model.DeliveredForReviewOrders[ i ].ClassCollection first last', false, 'DeliveredForReviewOrders')">
                        <td>
                            <div class="biggreen" id="arrowdiv @i">
                            </div>
                        </td>
                        <td>
                            <div class="@Model.DeliveredForReviewOrders[ i ].ExceptionClassCollection">
                            </div>
                        </td>
                        <td style="display: none" class="loanid">@Model.DeliveredForReviewOrders[ i ].LoanId</td>
                        <td style="display: none" class="encryptedloanid">@Server.UrlEncode( EncryptionHelper.EncryptRijndael( Model.DeliveredForReviewOrders[ i ].LoanId.ToString(), EncriptionKeys.Default ) )</td>
                        <td>
                            @Model.DeliveredForReviewOrders[ i ].LoanNumber
                        </td>
                        <td>
                            @Model.DeliveredForReviewOrders[ i ].Borrower
                        </td>
                        <td>
                            @Model.DeliveredForReviewOrders[ i ].Purpose.GetStringValue()
                        </td>
                        <td>
                            <div class="numbersrightloanamount">@Model.DeliveredForReviewOrders[ i ].LoanAmount.ToString().ToMoney()</div>
                        </td>
                        <td>
                            @if (Model.DeliveredForReviewOrders[i].Purpose == LoanTransactionType.Refinance || ( Model.DeliveredForReviewOrders[ i ].PurchaseAmount != null && Model.DeliveredForReviewOrders[ i ].PurchaseAmount.Equals( 0 ) ) )
                            {
                                <div>
                                </div>
                            }
                            else
                            {
                                <div class="numbersrightloanamount">@Model.DeliveredForReviewOrders[ i ].PurchaseAmount.ToString().ToMoney()</div>
                            }
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].EstimatedValue != null && Model.DeliveredForReviewOrders[ i ].EstimatedValue.Equals( 0 ) )
                            {
                                <div>
                                </div>
                            }
                            else
                            {
                                <div class="numbersrightloanamount">@Model.DeliveredForReviewOrders[ i ].EstimatedValue.ToString().ToMoney()</div>
                            }
                        </td>
                        <td class="paddingL15">
                            @( Model.DeliveredForReviewOrders[ i ].OnLine ? "Yes" : "No" )
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].ValueSupported )
                            {
                                <img src="@CDNHelper.ImagesStaticContentUrl/Content/images/okhandgreen.png" />
                            }
                            else
                            {
                                <div>
                                </div>
                            }
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].eConsented )
                            {
                                <img src="@CDNHelper.ImagesStaticContentUrl/Content/images/okhandgreen.png" />
                            }
                            else
                            {
                                <div>
                                </div>
                            }
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].eDelivered )
                            {
                                <img src="@CDNHelper.ImagesStaticContentUrl/Content/images/okhandgreen.png" />
                            }
                            else
                            {
                                <div>
                                </div>
                            }
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].UploadedDate != null && Model.DeliveredForReviewOrders[ i ].UploadedDate.HasValue )
                            {
                                @Model.DeliveredForReviewOrders[ i ].UploadedDate.Value.ToString( "MM/dd/yyyy" );
                            }
                        </td>
                        <td>
                            @if ( Model.DeliveredForReviewOrders[ i ].DeliveredDate != null && Model.DeliveredForReviewOrders[ i ].DeliveredDate.HasValue )
                            {
                                @Model.DeliveredForReviewOrders[ i ].DeliveredDate.Value.ToString( "MM/dd/yyyy" );
                            }
                        </td>
                        <td>
                            @Model.DeliveredForReviewOrders[ i ].Age
                        </td>
                    </tr>
                    }
                }
            </table>
        </div>
        <div class="gridlistfooter">
            <div id="numberOfRecordsDiv" class="normalfont left142" style="width: 170px;">
                @if ( Model.TotalItems != 0 )
                {     
                    @(( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage * 10 - 9) <span>-</span> @(( ( MML.Web.LoanCenter.Helpers.Utilities.OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage * 10 - ( 10 - @Model.DeliveredForReviewOrders.Count )) <span>of</span> @Model.TotalItems.ToString() <span>records</span>
                }
                else if ( Model.TotalItems == 0 )
                {
                    <span>0 records</span>
                }
            </div>
            @if ( Model.PageCount > 1 )
            { 
                <div id="pagingDiv" class="normalfont left20">
                    @if ( Model.TotalItems != 0 )
                    {
                        if ( ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage != 1 )
                        {               
                        <a href="javascript:void(0)" onclick="OrderDeliveredForReviewGrid.DataHelper('command=OrderDeliveredForReviewGridPaging,Page=@(( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage - 1)')">
                            <div class="leftarrow">
                            </div>
                        </a>
                        }
                        else
                        {
                        <div class="leftarrow">
                        </div>
                        }
                        <div class="left1010">
                            @if ( Model.CurrentPage > 10 )
                            {
                                @Ajax.ActionLink( "...", "Execute", "Command", new
                           {
                               Command = String.Format( "OrderDeliveredForReviewGridPaging,Page={0}",
                                   ( Model.PageCount >= ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage - 10 ) ?
                                   ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage - 10 :
                                   Model.PageCount )
                           },
                                                                                                new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Page, plusTen')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Page, plusTen')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Page, plusTen')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Page, plusTen')" }, new { @class = "page" } )
                            }
                            @for ( int j = Model.StartPage; j <= Model.EndPage; j++ )
                            {
                                if ( j != ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage )
                                { 
                                @Ajax.ActionLink( String.Format( "{0} ", j ), "Execute", "Command", new { Command = String.Format( "OrderDeliveredForReviewGridPaging,Page={0}", j ) },
                                                                          new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Page{0}')", j ), OnComplete = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Page{0}')", j ), OnBegin = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Page{0}')", j ), OnSuccess = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Page{0}')", j ) }, new { @class = "page" } )
                                }
                                else
                                { 
                                @Ajax.ActionLink( String.Format( "{0} ", j ), "Execute", "Command", new { Command = String.Format( "OrderDeliveredForReviewGridPaging,Page={0}", j ) }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Page{0}')", j ), OnComplete = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Page{0}')", j ), OnBegin = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Page{0}')", j ), OnSuccess = String.Format( "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Page{0}')", j ) }, new { @class = "pageselected" } )
                                }
                            }
                            @if ( Model.LastPageDots == false )
                            {
                                @Ajax.ActionLink( "...", "Execute", "Command", new
                           {
                               Command = String.Format( "OrderDeliveredForReviewGridPaging,Page={0}",
                               ( Model.PageCount >= ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage + 10 ) ?
                               ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage + 10 : Model.PageCount )
                           },
                                                                                        new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnFailure('Page, plusTen')", OnComplete = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnComplete('Page, plusTen')", OnBegin = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnBegin('Page, plusTen')", OnSuccess = "OrderDeliveredForReviewGrid.OrderDeliveredForReviewDataOnSuccess('Page, plusTen')" }, new { @class = "page" } )
                            }
                        </div>
                            if ( ( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage != Model.PageCount )
                            {              
                        <a href="javascript:void(0)" onclick="OrderDeliveredForReviewGrid.DataHelper('command=OrderDeliveredForReviewGridPaging,Page=@(( ( OrderDeliveredForReviewListState )HttpContext.Current.Session[ SessionHelper.OrderDeliveredForReviewListState ] ).CurrentPage + 1)')">
                            <div class="rightarrow">
                            </div>
                        </a>
                            }
                            else
                            { 
                        <div class="rightarrow">
                        </div>
                            }
                    }
                </div>
            }
        </div>
    </div>
</div>
