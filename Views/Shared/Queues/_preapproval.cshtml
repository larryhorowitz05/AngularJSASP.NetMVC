@model MML.Web.LoanCenter.ViewModels.PreApprovalViewModel
@using MML.Contracts
@using Telerik.Web.Mvc.UI
@using MML.Common
@using MML.Common.Helpers
@using MML.Web.LoanCenter.Helpers.Utilities
<div class="todolist">
    <div id="todolisthead" class="todolisthead">
        <div class="left0 w180">
            <div class="fleft">
                <span id="currentWorkQueueType" class="namelink">PreApproval</span></div>
            <div class="smallarrow">
            </div>
        </div>
        <div class="nooftasks">
            <span id="nooftasks2">@Model.TotalItems.ToString()</span> <span class="font15">Pre-Approval
                items</span>
        </div>
        <div class="right1">
            <div class="left1">
                <span class="normalfont">Show: </span><span id="officerTaskHeadderSpan" class="g12">@MML.Common.Helpers.EnumHelper.GetStringValue( ( ( MML.Web.LoanCenter.Helpers.Utilities.PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).BoundDate )</span>
            </div>
            <div class="right0">
                <div class="filter">
                    <ul id="nav">
                        <li><a href="#">&nbsp;</a>
                            <ul>
                                <li></li>
                                <li id="ftoday" class="lower">@Ajax.ActionLink( "Today", "Execute", "Command", new { Command = "PreApprovalGridDateFilter,DateFilter=1" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Today')" }, new { id = "aftoday" } )</li>
                                <li id="ftomorrow" class="lower">@Ajax.ActionLink( "Yesterday", "Execute", "Command", new { Command = "PreApprovalGridDateFilter,DateFilter=2" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Tomorrow')" }, new { id = "aftomorrow" } )</li>
                                <li id="fthisweek" class="lower">@Ajax.ActionLink( "Last Week", "Execute", "Command", new { Command = "PreApprovalGridDateFilter,DateFilter=3" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('This Week')" }, new { id = "afthisweek" } )</li>
                                <li id="fnextweek" class="lower">@Ajax.ActionLink( "Last Month", "Execute", "Command", new { Command = "PreApprovalGridDateFilter,DateFilter=4" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Next Week')" }, new { id = "afnextweek" } )</li>
                                <li id="fthismonth" class="lower">@Ajax.ActionLink( "All Open", "Execute", "Command", new { Command = "PreApprovalGridDateFilter,DateFilter=5" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('This Month')" }, new { id = "afthismonth" } )</li>
                                <li></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <a id="minimizetoday">
                    <div id="minmax" class="min">
                    </div>
                </a>
            </div>
        </div>
    </div>
    <div id="listandpage">
        <div class="preapprovalcontent">
            <table class="preapprovaltable">
                <tr class="preapprovalsubjects" id="trGridHeader">
                    <td>
                    </td>
                    <td class="t0">
                    </td>
                    <td style="display: none">
                    </td>
                    <td class="t1">@Ajax.ActionLink( "Concierge", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=1" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('ConciergeFullName')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('ConciergeFullName')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('ConciergeFullName')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('ConciergeFullName')" } )
                    </td>
                    <td class="t9">@Ajax.ActionLink( "Docs", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=2" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('NumberOfDocs')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('NumberOfDocs')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('NumberOfDocs')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('NumberOfDocs')" } )
                    </td>
                    <td class="t2">@Ajax.ActionLink( "Loan Number", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=3" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('LoanNumber')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('LoanNumber')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('LoanNumber')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('LoanNumber')" } )
                    </td>
                    <td class="t8">@Ajax.ActionLink( "Request Date", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=4" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('Date')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('Date')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('Date')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Date')" } )
                    </td>
                    <td class="t3">@Ajax.ActionLink( "Borrower", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=5" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('BorrowerFullName')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('BorrowerFullName')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('BorrowerFullName')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('BorrowerFullName')" } )
                    </td>
                    <td class="t4">@Ajax.ActionLink( "Program Name", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=6" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('ProgramName')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('ProgramName')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('ProgramName')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('ProgramName')" } )
                    </td>
                    <td class="t5">@Ajax.ActionLink( "Amount", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=7" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('Amount')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('Amount')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('Amount')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Amount')" } )
                    </td>
                    <td class="t6">@Ajax.ActionLink( "Rate", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=8" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('Rate')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('Rate')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('Rate')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Rate')" } )
                    </td>
                    <td class="t7">@Ajax.ActionLink( "PreApproval", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=9" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('LockDate')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('LockDate')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('LockDate')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('LockDate')" } )
                    </td>
                    <td class="t10">@Ajax.ActionLink( "Current Activity", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=10" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('ActivityName')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('ActivityName')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('ActivityName')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('ActivityName')" } )
                    </td>
                    <td class="t10">@Ajax.ActionLink( "Activity Status", "Execute", "Command", new { Command = "PreApprovalGridSorting,Column=11" }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('ActivityName')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('ActivityName')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('ActivityName')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('ActivityName')" } )
                    </td>
                </tr>
                @if ( Model.PreApprovalItems != null )
                {
                    for ( int i = 0; i < Model.PreApprovalItems.Count; i++ )
                    {
                        if ( Model.PreApprovalItems[ i ].PreApprovalViewItems.Count > 0 )
                        {
                    <tr class="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ClassCollection"  id="task_@{@String.Format( "{0}P", i )}" onclick="SelectedTask('@String.Format( "{0}P", i )','@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ClassCollection', false, 'PreApproval')">
                        <td>
                            <div class="biggreen" id="arrowdiv_@{@String.Format( "{0}P", i )}">
                                
                            </div>
                            @if ( Model.PreApprovalItems[ i ].PreApprovalViewItems.Count > 1 )
                            {
                                <div class="childIndicator" id="childIndicator_@{@String.Format( "{0}P", i )}" onclick="$(this).unbind('click'); SelectedTask('@String.Format( "{0}P", i )','@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ClassCollection', false, 'PreApproval')">
                                
                                </div>  
                            }
                        </td>
                        <td>
                            <div class="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ExceptionClassCollection exceptionIcon">
                            </div>
                        </td>
                        <td style="display: none" class="loanid">@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LoanId</td>
                        <td style="display: none" class="encryptedloanid">@Server.UrlEncode( EncryptionHelper.EncryptRijndael( Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LoanId.ToString(), EncriptionKeys.Default ) )</td>
                        <td>
                            <span class="conciergeName @Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LicenseExpiredClass" title="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LicenseExpiredMessage">
                                @Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ConciergeFullName
                            </span>
                        </td>
                        <td class="textcenter">
                            @if ( Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].NumberOfDocs == -1 )
                            {
                                <div class="eDisclosureNotSigned" title="eConsent Pending"></div>
                            }
                            else
                            {
                                @Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].NumberOfDocs
                            }
                        </td>
                        <td class="textcenter">@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LoanNumber
                        </td>
                        <td>@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalRequestDate.ToString( "MM/dd/yyyy" )
                        </td>
                        <td>@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].BorrowerFullName
                        </td>
                        <td>
                            <span title="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ProgramName">@(Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ProgramName.Length > 25 ? String.Format( "{0}...", Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ProgramName.Substring( 0, 23 ) )
: Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ProgramName)
                            </span>
                        </td>
                        <td>
                            <div class="numbersright">@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].MortgageAmount.ToString().ToMoney()</div>
                        </td>
                        <td>
                            <div class="numbersright">@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].Rate.ToString( "n3" )
                                %</div>
                        </td>
                        <td>
                            @if ( ( Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalStatus == PreApprovalStatus.Issued.ToString() || ( Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalStatus == PreApprovalStatus.Reissued.ToString() ) && Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalLetterRepositoryItemId != Guid.Empty ) )
                            {
                                string downloanLink = string.Format( "Downloader.axd?documentType=repositoryItem&repositoryItemId={0}", @Server.UrlEncode( EncryptionHelper.EncryptRijndael( Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalLetterRepositoryItemId.ToString(), EncriptionKeys.Default ) ) );
                                <div id='letterTooltipDiv' class='letterTooltipDiv' expirationdate=@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].LetterExpirationDate>
                                    <div class='preApprovalLetterIconText'>
                                        <span>@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalStatus</span></div>
                                    <div class='preApprovalLetterIconLink'>
                                        <a class='preApprovalGridLetterIcon' href=@downloanLink/>
                                    </div>
                                </div>
                            }
                            else
                            {
                                @Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].PreApprovalStatus.ToString();
                            }
                        </td>
                        <td>@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ActivityName
                        </td>
                        <td>@Model.PreApprovalItems[ i ].PreApprovalViewItems[ 0 ].ActivityStatus
                        </td>
                        <td style="display: none" class="activityId">@Model.PreApprovalItems[i].PreApprovalViewItems[0].ActivityTypeId </td>
                    </tr>
                    	
                        @*Render childs loans*@
                        for ( int j = 1; j < Model.PreApprovalItems[ i ].PreApprovalViewItems.Count; j++ )
                        { 
                            <tr class="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].ClassCollection childloan"  id="task_@{@String.Format( "{0}P{1}", i, j )}" onclick="SelectedTask('@String.Format( "{0}P", i )','@Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].ClassCollection', true, 'PreApproval')" style="display:none">
                                <td>
                                </td>
                                <td>
                                    <div class="@Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].ExceptionClassCollection">
                                    </div>
                                </td>
                                <td style="display: none" class="loanid">@Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].LoanId
                                </td>
                                <td class="textcenter">
                                    @*Concierge Name*@
                                </td>
                                <td class="textcenter">
                                    @if ( Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].NumberOfDocs == -1 )
                                    {
                                        <div class="eDisclosureNotSigned" title="eSignDisclosure Not Signed">
                                        </div>
                                    }
                                    else
                                    {
                                        @Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].NumberOfDocs
                                    }
                                </td>
                                <td>
                                    @*Loan Number*@
                                </td>
                                <td>
                                    @*Request date*@
                                </td>
                                <td>
                                    @Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].BorrowerFullName
                                </td>
                                <td>
                                    @*Program Name*@
                                </td>
                                <td>
                                    @*Amount*@
                                </td>
                                <td>
                                    @*Rate*@
                                </td>
                                <td>
                                    @*Preapproval Status*@
                                </td>
                                <td>
                                    @Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].ActivityName
                                </td>
                                <td>
                                    @Model.PreApprovalItems[ i ].PreApprovalViewItems[ j ].ActivityStatus
                                </td>
                            </tr>
                        }
                        }
                    }

                }
            </table>
        </div>
        <div class="todolistfooter">
            <div id="numberOfRecordsDiv" class="normalfont left142" style="width: 170px;">
                @if ( Model.TotalItems != 0 )
                {     
                    @(( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage * 10 - 9) <span>-</span> @(( ( MML.Web.LoanCenter.Helpers.Utilities.PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage * 10 - ( 10 - @Model.PreApprovalItems.Count )) <span>of</span> @Model.TotalItems.ToString() <span>records</span>
                }
                else if ( Model.TotalItems == 0 )
                {
                    <span>0 records</span>
                }
            </div>
            @if ( Model.PageCount > 1 )
            { 
                <div id="pagingDiv" class="normalfont left20">
                    @if ( Model.TotalItems != 0 )
                    {
                        if ( ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage != 1 )
                        {               
                        <a href="javascript:void(0)" onclick="PreApprovalGrid.PreApprovalDataHelper('command=PreApprovalGridPaging,Page=@(( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage - 1)')">
                            <div class="leftarrow">
                            </div>
                        </a>
                        }
                        else
                        {
                        <div class="leftarrow">
                        </div>
                        }
                        <div class="left1010">
                            @if ( Model.CurrentPage > 10 )
                            {
                                @Ajax.ActionLink( "...", "Execute", "Command", new
                           {
                               Command = String.Format( "PreApprovalGridPaging,Page={0}",
                                 ( Model.PageCount >= ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage - 10 ) ?
                                 ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage - 10 :
                                 Model.PageCount )
                           },
                                                                                                       new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('Page, plusTen')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('Page, plusTen')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('Page, plusTen')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Page, plusTen')" }, new { @class = "page" } )
                            }
                            @for ( int j = Model.StartPage; j <= Model.EndPage; j++ )
                            {
                                if ( j != ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage )
                                { 
                                @Ajax.ActionLink( String.Format( "{0} ", j ), "Execute", "Command", new { Command = String.Format( "PreApprovalGridPaging,Page={0}", j ) },
                                                                                                                                                      new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = String.Format( "PreApprovalGrid.PreApprovalDataOnFailure('Page{0}')", j ), OnComplete = String.Format( "PreApprovalGrid.PreApprovalDataOnComplete('Page{0}')", j ), OnBegin = String.Format( "PreApprovalGrid.PreApprovalDataOnBegin('Page{0}')", j ), OnSuccess = String.Format( "PreApprovalGrid.PreApprovalDataOnSuccess('Page{0}')", j ) }, new { @class = "page" } )
                                }
                                else
                                { 
                                @Ajax.ActionLink( String.Format( "{0} ", j ), "Execute", "Command", new { Command = String.Format( "PreApprovalGridPaging,Page={0}", j ) }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = String.Format( "PreApprovalGrid.PreApprovalDataOnFailure('Page{0}')", j ), OnComplete = String.Format( "PreApprovalGrid.PreApprovalDataOnComplete('Page{0}')", j ), OnBegin = String.Format( "PreApprovalGrid.PreApprovalDataOnBegin('Page{0}')", j ), OnSuccess = String.Format( "PreApprovalGrid.PreApprovalDataOnSuccess('Page{0}')", j ) }, new { @class = "pageselected" } )
                                }
                            }
                            @if ( Model.LastPageDots == false )
                            {
                                @Ajax.ActionLink( "...", "Execute", "Command", new
                           {
                               Command = String.Format( "PreApprovalGridPaging,Page={0}",
                               ( Model.PageCount >= ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage + 10 ) ?
                               ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage + 10 :
                               Model.PageCount )
                           },
                                                                                                       new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "mainsection", OnFailure = "PreApprovalGrid.PreApprovalDataOnFailure('Page, plusTen')", OnComplete = "PreApprovalGrid.PreApprovalDataOnComplete('Page, plusTen')", OnBegin = "PreApprovalGrid.PreApprovalDataOnBegin('Page, plusTen')", OnSuccess = "PreApprovalGrid.PreApprovalDataOnSuccess('Page, plusTen')" }, new { @class = "page" } )
                            }
                        </div>
                            if ( ( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage != Model.PageCount )
                            {              
                        <a href="javascript:void(0)" onclick="PreApprovalGrid.PreApprovalDataHelper('command=PreApprovalGridPaging,Page=@(( ( PreApprovalListState )HttpContext.Current.Session[ SessionHelper.PreApprovalListState ] ).CurrentPage + 1)')">
                            <div class="rightarrow">
                            </div>
                        </a>
                            }
                            else
                            { 
                        <div class="rightarrow">
                        </div>
                            }
                    }
                </div>
            }
        </div>
    </div>
</div>
